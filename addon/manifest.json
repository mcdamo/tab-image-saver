{
  "manifest_version": 2,
  "name": "Tab Image Saver",
  "applications": {
    "gecko": {
      "strict_min_version": "57.0",
      "id": "tab-image-saver@mcdamo.addons.mozilla.org"
    }
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "downloads",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/down-16.png",
    "32": "icons/down-32.png",
    "48": "icons/down-48.png",
    "64": "icons/down-64.png",
    "96": "icons/down-96.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/down-16.png",
      "32": "icons/down-32.png",
      "48": "icons/down-48.png",
      "64": "icons/down-64.png",
      "96": "icons/down-96.png"
    },
    "default_title": "Tab Image Saver",
    "browser_style": true
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "version": "1.2.2",
  "homepage_url": "https://github.com/mcdamo/tab-image-saver"
}